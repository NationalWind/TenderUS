generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

enum Role {
  USER
  ADMIN
}

model Account {
  id       String @id @default(auto()) @map("_id") @db.ObjectId
  username String @unique
  password String
  role     Role
}
